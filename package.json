{
  "name": "translatium",
  "description": "Google Translate client for macOS & Windows",
  "version": "8.1.0",
  "main": "public/electron.js",
  "repository": "https://github.com/quanglam2807/translatium",
  "author": {
    "name": "Quang Lam",
    "email": "quang.lam2807@gmail.com",
    "url": "https://quanglam2807.github.io"
  },
  "license": "MPL-2.0",
  "engines": {
    "node": "8"
  },
  "scripts": {
    "start": "cross-env REACT_APP_GOOGLE_ENDPOINT=http://localhost:3000 BROWSER=none react-scripts start",
    "electron-dev": "concurrently \"yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "react-scripts build && node add-bom.js",
    "test-ui": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "dist-electron": "npm run build && babel-node packager.js",
    "lint": "eslint ./src ./public ./packager.js ./add-bom.js --ext js --fix"
  },
  "dependencies": {
    "electron-is-dev": "0.3.0",
    "electron-settings": "3.1.4",
    "menubar": "5.2.3"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.6.1",
    "classnames": "2.2.5",
    "concurrently": "3.5.1",
    "cross-env": "5.1.4",
    "del": "3.0.0",
    "electron": "1.8.3",
    "electron-builder": "20.4.1",
    "eslint": "4.18.2",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "glob": "7.1.2",
    "history": "4.7.2",
    "material-ui": "1.0.0-beta.36",
    "material-ui-icons": "1.0.0-beta.36",
    "mousetrap": "1.6.1",
    "pouchdb-browser": "6.4.3",
    "prop-types": "15.6.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.7",
    "react-router": "3.0.5",
    "react-router-redux": "4.0.8",
    "react-scripts": "1.1.1",
    "recordrtc": "5.4.6",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "typeface-roboto": "0.0.54",
    "wait-on": "2.1.0"
  },
  "homepage": "./",
  "proxy": {
    "/m/translate": {
      "target": "https://translate.google.com",
      "changeOrigin": true
    },
    "/translate_a": {
      "target": "https://translate.google.com",
      "changeOrigin": true
    },
    "/translate_tts": {
      "target": "https://translate.google.com",
      "changeOrigin": true,
      "ws": true
    },
    "/speech-api": {
      "target": "https://www.google.com",
      "changeOrigin": true
    }
  },
  "private": true
}
