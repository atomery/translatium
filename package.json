{
  "name": "Translatium",
  "description": "Translate Any Languages like a Pro",
  "version": "10.2.2",
  "main": "public/electron.js",
  "repository": "https://github.com/translatium/translatium",
  "author": {
    "name": "Quang Lam",
    "email": "quang.lam2807@gmail.com"
  },
  "license": "MPL-2.0",
  "engines": {
    "node": "12"
  },
  "scripts": {
    "postinstall": "npx patch-package",
    "start": "cross-env REACT_APP_GOOGLE_ENDPOINT=http://localhost:3000 BROWSER=none react-scripts start",
    "electron-dev": "concurrently \"yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "react-scripts build",
    "test-ui": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "dist": "yarn run build && babel-node packager.js",
    "lint": "eslint ./src ./public ./packager.js --ext js",
    "test": "mocha",
    "localize": "npx babel-node localizer.js"
  },
  "dependencies": {
    "cli-truncate": "2.1.0",
    "electron-is-dev": "1.1.0",
    "electron-settings": "3.2.0",
    "electron-updater": "4.2.5",
    "menubar": "6.0.8"
  },
  "devDependencies": {
    "@material-ui/core": "4.9.7",
    "@material-ui/icons": "4.9.1",
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "blueimp-md5": "2.12.0",
    "chai": "4.2.0",
    "classnames": "2.2.6",
    "concurrently": "5.1.0",
    "cross-env": "7.0.2",
    "del": "5.1.0",
    "dotenv": "8.2.0",
    "electron": "6.1.9",
    "electron-builder": "22.4.1",
    "electron-notarize": "0.2.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "fs-extra": "8.1.0",
    "glob": "7.1.6",
    "mocha": "7.1.1",
    "node-fetch": "2.6.0",
    "pouchdb-browser": "7.2.1",
    "prop-types": "15.7.2",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-redux": "7.2.0",
    "react-scripts": "3.4.0",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "tongwen-core": "3.2.1",
    "transliteration": "2.1.8",
    "typeface-roboto": "0.0.75",
    "wait-on": "4.0.1"
  },
  "homepage": "./",
  "private": true,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
